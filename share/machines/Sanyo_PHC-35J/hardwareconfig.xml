<?xml version="1.0"?>
<!-- $Id$ -->
<!DOCTYPE msxconfig SYSTEM "msxconfig.dtd">
<msxconfig>

  <info>
    <manufacturer>Sanyo</manufacturer>
    <code>PHC-35J</code>
    <release_year>1989</release_year>
    <description></description>
    <type>MSX2+</type>
  </info>

  <config id="ExternalSlots">
    <parameter name="slota">1-0</parameter>
    <parameter name="slotb">2-0</parameter>
  </config>

  <config id="MotherBoard">
    <parameter name="0" class="subslotted">false</parameter>
    <parameter name="1" class="subslotted">false</parameter>
    <parameter name="2" class="subslotted">false</parameter>
    <parameter name="3" class="subslotted">true</parameter>
  </config>

  <config id="RenShaTurbo">
     <parameter name="min_ints">47</parameter>
     <parameter name="max_ints">221</parameter>
  </config>

  <config id="MapperType">
    <parameter name="type">Philips</parameter>
  </config>

  <device id="Main RAM">
    <type>MemoryMapper</type>
    <slotted> <ps>3</ps> <ss>0</ss> <page>0</page> </slotted>
    <slotted> <ps>3</ps> <ss>0</ss> <page>1</page> </slotted>
    <slotted> <ps>3</ps> <ss>0</ss> <page>2</page> </slotted>
    <slotted> <ps>3</ps> <ss>0</ss> <page>3</page> </slotted>
    <parameter name="slow_drain_on_reset">false</parameter>
    <parameter name="size">64</parameter>
  </device>

  <device id="MSX BIOS with BASIC ROM">
    <type>Rom</type>
    <slotted> <ps>0</ps> <ss>0</ss> <page>0</page> </slotted>
    <slotted> <ps>0</ps> <ss>0</ss> <page>1</page> </slotted>
    <parameter name="filename">roms/phc-35j_basic-bios2p.rom</parameter>
  </device>

  <device id="MSX Sub ROM">
    <type>Rom</type>
    <slotted> <ps>3</ps> <ss>1</ss> <page>0</page> </slotted>
    <parameter name="filename">roms/phc-35j_msx2psub.rom</parameter>
  </device>

  <device id="MSX Kanji Driver with BASIC">
    <type>Rom</type>
    <slotted> <ps>3</ps> <ss>1</ss> <page>1</page> </slotted>
    <slotted> <ps>3</ps> <ss>1</ss> <page>2</page> </slotted>
    <parameter name="filename">roms/phc-35j_kanjibasic.rom</parameter>
  </device>

  <device id="Kanji ROM">
    <type>Kanji</type>
    <parameter name="filename">roms/phc-35j_kanjifont.rom</parameter>
  </device>

  <device id="ppi">
    <type>PPI</type>
    <parameter name="volume">16000</parameter>
    <parameter name="key_ghosting">true</parameter>
  </device>

  <device id="VDP">
    <type>VDP</type>
    <parameter name="version">V9958</parameter>
    <parameter name="vram">128</parameter>
  </device>

  <device id="PSG">
    <type>PSG</type>
    <parameter name="volume">21000</parameter>
    <parameter name="keyboardlayout">JIS</parameter>
  </device>

  <device id="Real time clock">
    <type>RTC</type>
    <parameter name="mode">EmuTime</parameter>
    <parameter name="sramname">phc-35j.cmos</parameter>
  </device>

  <device id="cpu">
    <type>CPU</type>
  </device>

  <device id="Printer Port">
    <type>PrinterPort</type>
  </device>

  <device id="F4Device">
    <type>F4Device</type>
    <parameter name="inverted">true</parameter>
  </device>

  <config id="CassettePort">
  </config>

</msxconfig>
