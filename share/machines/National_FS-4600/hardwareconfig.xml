<?xml version="1.0"?>
<!DOCTYPE msxconfig SYSTEM "msxconfig.dtd">
<msxconfig>

  <config id="ExternalSlots">
    <parameter name="slota">1-0</parameter>
    <parameter name="slotb">2-0</parameter>
  </config>

  <config id="MotherBoard">
    <parameter name="0" class="subslotted">true</parameter>
    <parameter name="1" class="subslotted">false</parameter>
    <parameter name="2" class="subslotted">false</parameter>
    <parameter name="3" class="subslotted">true</parameter>
  </config>

  <device id="MapperIO">
    <type>MapperIO</type>
    <parameter name="type">Philips</parameter>
  </device>

  <device id="Main RAM">
    <type>MemoryMapper</type>
    <slotted> <ps>3</ps> <ss>2</ss> <page>0</page> </slotted>
    <slotted> <ps>3</ps> <ss>2</ss> <page>1</page> </slotted>
    <slotted> <ps>3</ps> <ss>2</ss> <page>2</page> </slotted>
    <slotted> <ps>3</ps> <ss>2</ss> <page>3</page> </slotted>
    <parameter name="slow_drain_on_reset">false</parameter>
    <parameter name="size">128</parameter>
  </device>

  <device id="MSX BIOS with BASIC ROM">
    <type>Rom</type>
    <slotted> <ps>0</ps> <ss>0</ss> <page>0</page> </slotted>
    <slotted> <ps>0</ps> <ss>0</ss> <page>1</page> </slotted>
    <parameter name="filename">fs-4600_basic-bios2.rom</parameter>
  </device>

  <device id="MSX Sub ROM">
    <type>Rom</type>
    <slotted> <ps>0</ps> <ss>1</ss> <page>0</page> </slotted>
    <parameter name="filename">fs-4600_msx2sub.rom</parameter>
  </device>

  <device id="Word Font 1">
    <type>Rom</type>
    <slotted> <ps>0</ps> <ss>2</ss> <page>0</page> </slotted>
    <parameter name="filename">fs-4600_wordfont1.rom</parameter>
  </device>

  <device id="Word Font 2">
    <type>Rom</type>
    <slotted> <ps>0</ps> <ss>3</ss> <page>0</page> </slotted>
    <parameter name="filename">fs-4600_wordfont2.rom</parameter>
  </device>

  <device id="MSX Word">
    <type>Rom</type>
    <slotted> <ps>3</ps> <ss>1</ss> <page>0</page> </slotted>
    <slotted> <ps>3</ps> <ss>1</ss> <page>1</page> </slotted>
    <slotted> <ps>3</ps> <ss>1</ss> <page>2</page> </slotted>
    <slotted> <ps>3</ps> <ss>1</ss> <page>3</page> </slotted>
    <parameter name="filename">fs-4600_msxword.rom</parameter>
    <parameter name="mappertype">NATIONAL</parameter>
    <parameter name="loadsram">true</parameter>
    <parameter name="savesram">true</parameter>
    <parameter name="sramname">fs-4600.sram</parameter>
  </device>

  <device id="MSX Kanji Driver">
    <type>Rom</type>
    <slotted> <ps>0</ps> <ss>2</ss> <page>1</page> </slotted>
    <slotted> <ps>0</ps> <ss>2</ss> <page>2</page> </slotted>
    <parameter name="filename">fs-4600_kanjibasic.rom</parameter>
  </device>

  <device id="Kanji ROM">
    <type>Kanji</type>
    <parameter name="filename">fs-4600_kanjifont.rom</parameter>
  </device>

  <device id="Kanji ROM 12X12">
    <type>Kanji12</type>
    <parameter name="filename">fs-4600_kanjifont12.rom</parameter>
  </device>

  <device id="Memory Mapped FDC">
    <type>FDC</type>
    <slotted> <ps>3</ps> <ss>3</ss> <page>1</page> </slotted>
    <slotted> <ps>3</ps> <ss>3</ss> <page>2</page> </slotted>
    <parameter name="filename">fs-4600_disk.rom</parameter>
    <parameter name="type">National</parameter>
    <parameter name="brokenFDCread">false</parameter>
    <parameter name="drivename1">diska</parameter>
  </device>

  <device id="S1985">
    <type>S1985</type>
  </device>

  <device id="ppi">
    <type>PPI</type>
    <parameter name="volume">16000</parameter>
    <parameter name="key_ghosting">true</parameter>
  </device>

  <device id="VDP">
    <type>VDP</type>
    <parameter name="version">V9938</parameter>
    <parameter name="vram">128</parameter>
  </device>

  <device id="psg">
    <type>PSG</type>
    <parameter name="volume">21000</parameter>
  </device>

  <device id="Real time clock">
    <type>RTC</type>
    <parameter name="mode">EmuTime</parameter>
    <parameter name="loadsram">true</parameter>
    <parameter name="savesram">true</parameter>
    <parameter name="sramname">fs-4600.cmos</parameter>
  </device>

  <device id="cpu">
    <type>CPU</type>
  </device>

  <device id="Printer Port">
    <type>PrinterPort</type>
  </device>

</msxconfig>
