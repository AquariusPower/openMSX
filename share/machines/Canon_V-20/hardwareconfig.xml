<?xml version="1.0"?>
<!-- $Id$ -->
<!DOCTYPE msxconfig SYSTEM "msxconfig.dtd">
<msxconfig>

<config id="ExternalSlots">
    <parameter name="slota">1-0</parameter>
    <parameter name="slotb">3-0</parameter>
  </config>

  <config id="MotherBoard">
    <parameter name="0" class="subslotted">false</parameter>
    <parameter name="1" class="subslotted">false</parameter>
    <parameter name="2" class="subslotted">false</parameter>
    <parameter name="3" class="subslotted">false</parameter>
  </config>

  <device id="Main RAM">
    <type>RAM</type>
    <slotted> <ps>2</ps> <ss>0</ss> <page>0</page> </slotted>
    <slotted> <ps>2</ps> <ss>0</ss> <page>1</page> </slotted>
    <slotted> <ps>2</ps> <ss>0</ss> <page>2</page> </slotted>
    <slotted> <ps>2</ps> <ss>0</ss> <page>3</page> </slotted>
    <parameter name="slow_drain_on_reset">false</parameter>
  </device>

  <device id="MSX BIOS with BASIC ROM">
    <type>Rom</type>
    <slotted> <ps>0</ps> <ss>0</ss> <page>0</page> </slotted>
    <slotted> <ps>0</ps> <ss>0</ss> <page>1</page> </slotted>
    <parameter name="filename">roms/v-20_basic-bios1.rom</parameter>
        <parameter name="0x00E1" class="patch">0xED</parameter>
    <parameter name="0x00E2" class="patch">0xFE</parameter>
    <parameter name="0x00E3" class="patch">0xC9</parameter>
    <parameter name="0x00E4" class="patch">0xED</parameter>
    <parameter name="0x00E5" class="patch">0xFE</parameter>
    <parameter name="0x00E6" class="patch">0xC9</parameter>
    <parameter name="0x00E7" class="patch">0xED</parameter>
    <parameter name="0x00E8" class="patch">0xFE</parameter>
    <parameter name="0x00E9" class="patch">0xC9</parameter>
    <parameter name="0x00EA" class="patch">0xED</parameter>
    <parameter name="0x00EB" class="patch">0xFE</parameter>
    <parameter name="0x00EC" class="patch">0xC9</parameter>
    <parameter name="0x00ED" class="patch">0xED</parameter>
    <parameter name="0x00EE" class="patch">0xFE</parameter>
    <parameter name="0x00EF" class="patch">0xC9</parameter>
    <parameter name="0x00F0" class="patch">0xED</parameter>
    <parameter name="0x00F1" class="patch">0xFE</parameter>
    <parameter name="0x00F2" class="patch">0xC9</parameter>
    <parameter name="0x00F3" class="patch">0xED</parameter>
    <parameter name="0x00F4" class="patch">0xFE</parameter>
    <parameter name="0x00F5" class="patch">0xC9</parameter>
    <parameter name="" class="patchcode">MSXTapePatch</parameter>
  </device>

  <device id="ppi">
    <type>PPI</type>
    <parameter name="volume">16000</parameter>
    <parameter name="key_ghosting">true</parameter>
  </device>

  <device id="VDP">
    <type>VDP</type>
    <parameter name="version">TMS9929A</parameter>
  </device>

  <device id="psg">
    <type>PSG</type>
    <parameter name="volume">21000</parameter>
  </device>

  <device id="cpu">
    <type>CPU</type>
  </device>

  <device id="Printer Port">
    <type>PrinterPort</type>
  </device>

  <config id="CassettePort">
  </config>

</msxconfig>

