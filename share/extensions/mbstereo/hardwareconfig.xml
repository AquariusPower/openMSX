<?xml version="1.0"?>
<!DOCTYPE msxconfig SYSTEM "msxconfig.dtd">
<msxconfig>
  <device id="FM PAC">
    <type>FM-PAC</type>
    <slotted><ps>-1</ps><ss>-1</ss><page>1</page></slotted>
    <parameter name="filename">roms/FMPAC.ROM</parameter>
    <parameter name="volume">13000</parameter>
    <parameter name="mode">left</parameter>
    <parameter name="loadsram">true</parameter>
    <parameter name="savesram">true</parameter>
    <parameter name="sramname">FMPAC.PAC</parameter>
  </device>
  <device id="MSX-Audio-1">
    <type>Audio</type>
    <parameter name="volume">12000</parameter>
    <parameter name="mode">right</parameter>
    <parameter name="sampleram">256</parameter>
  </device>
  <device id="MSX-Audio-Midi">
    <type>Audio-Midi</type>
  </device>
</msxconfig>
