<?xml version="1.0"?>
<!DOCTYPE msxconfig SYSTEM "msxconfig.dtd">
<msxconfig>
  <device id="FM PAC">
    <type>FM-PAC</type>
    <slotted><ps>-1</ps><ss>-1</ss><page>1</page></slotted>
    <parameter name="filename">roms/FMPAC.ROM</parameter>
    <parameter name="volume">13000</parameter>
    <parameter name="mode">mono</parameter>
    <parameter name="loadsram">true</parameter>
    <parameter name="savesram">true</parameter>
    <parameter name="sramname">FMPAC.PAC</parameter>
  </device>
</msxconfig>
