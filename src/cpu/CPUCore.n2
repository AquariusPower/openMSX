inline void _CPU_::SET_X(byte bit, byte& reg, word x) {
	reg = RDMEM(x);
	SET(bit, reg); 
	INC_DELAY();
	WRMEM(x, reg);
}
inline void _CPU_::SET_X_(byte bit, byte& reg, word x) { memptr.w = x; SET_X(bit, reg, x); }
inline void _CPU_::SET_X_X(byte bit, byte& reg) { SET_X_(bit, reg, R.IX.w + ofst); }
inline void _CPU_::SET_X_Y(byte bit, byte& reg) { SET_X_(bit, reg, R.IY.w + ofst); }

