// MULUB A,r
inline void _CPU_::MULUB(byte reg) {
	// TODO check flags
	clock += 12;
	R.HL.w = (word)R.AF.B.h * reg;
	R.AF.B.l = (R.AF.B.l & (N_FLAG | H_FLAG)) |
	           (R.HL.w ? 0 : Z_FLAG) |
		   ((R.HL.w & 0x8000) ? C_FLAG : 0);
}
