void _CPU_::mulub_a_l()   { } // TODO

// MULUW HL,ss
inline void _CPU_::MULUW(word reg) {
	// TODO check flags
	clock += 34;
	unsigned long res = (unsigned long)R.HL.w * reg;
	R.DE.w = res >> 16;
	R.HL.w = res & 0xffff;
	R.AF.B.l = (R.AF.B.l & (N_FLAG | H_FLAG)) |
	           (res ? 0 : Z_FLAG) |
		   ((res & 0x80000000) ? C_FLAG : 0);
}
