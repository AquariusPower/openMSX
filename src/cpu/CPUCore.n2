// ADC A,r
inline void _CPU_::ADC(byte reg) {
	int res = R.AF.B.h + reg + ((R.AF.B.l & C_FLAG) ? 1 : 0);
	R.AF.B.l = ZSXYTable[res & 0xFF] |
	           ((res & 0x100) ? C_FLAG : 0) |
	           ((R.AF.B.h ^ res ^ reg) & H_FLAG) |
	           (((reg ^ R.AF.B.h ^ 0x80) & (reg ^ res) & 0x80) ? V_FLAG : 0);
	R.AF.B.h = res;
}
