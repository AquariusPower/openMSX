// SBC HL,ss
inline void _CPU_::SBCW(word reg) {
	memptr.w = R.HL.w + 1;
	int res = R.HL.w - reg - ((R.AF.B.l & C_FLAG) ? 1 : 0);
	R.AF.B.l = (((R.HL.w ^ res ^ reg) >> 8) & H_FLAG) |
	           ((res & 0x10000) ? C_FLAG : 0) |
	           ((res & 0xffff) ? 0 : Z_FLAG) |
	           (((reg ^ R.HL.w) & (R.HL.w ^ res) & 0x8000) ? V_FLAG : 0) |
		   ((res >> 8) & (S_FLAG | X_FLAG | Y_FLAG)) |
	           N_FLAG;
	R.HL.w = res;
	OP_16_16_DELAY();
}
