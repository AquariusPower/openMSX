// DEC r
inline void _CPU_::DEC(byte &reg) {
	reg--;
	R.AF.B.l = (R.AF.B.l & C_FLAG) |
	           ((reg == 0x7f) ? V_FLAG : 0) |
	           (((reg & 0x0f) == 0x0f) ? H_FLAG : 0) |
	           ZSXYTable[reg] |
	           N_FLAG;
}
