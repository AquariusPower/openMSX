void _CPU_::adc_a_iyl() { ADC(R.IY.B.l); }
void _CPU_::adc_a_iyh() { ADC(R.IY.B.h); }
void _CPU_::adc_a_byte(){
	byte n = RDMEM_OPCODE(R.PC.w++);
	ADC(n);
}
inline void _CPU_::adc_a_x(word x) {
	byte n = RDMEM(x);
	ADC(n);
}
