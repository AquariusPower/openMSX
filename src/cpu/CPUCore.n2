// CP r
inline void _CPU_::CP(byte reg) {
	int q = R.AF.B.h - reg;
	R.AF.B.l = ZSTable[q & 0xFF] |
	           (reg & (X_FLAG | Y_FLAG)) |	// XY from operand, not from result
	           ((q & 0x100) ? C_FLAG : 0) |
	           N_FLAG |
	           ((R.AF.B.h ^ q ^ reg) & H_FLAG) |
	           (((reg ^ R.AF.B.h) & (R.AF.B.h ^ q) & 0x80) ? V_FLAG : 0);
}
