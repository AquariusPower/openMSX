// LD A,I/R
void _CPU_::ld_a_i() {
	SMALL_DELAY();
	R.AF.B.h = R.I;
	R.AF.B.l = (R.AF.B.l & C_FLAG) |
	           ZSXYTable[R.AF.B.h] |
		   (R.IFF2 ? V_FLAG : 0);
}
void _CPU_::ld_a_r() {
	SMALL_DELAY();
	R.AF.B.h = (R.R & 0x7f) | (R.R2 & 0x80);
	R.AF.B.l = (R.AF.B.l & C_FLAG) |
	           ZSXYTable[R.AF.B.h] |
		   (R.IFF2 ? V_FLAG : 0);
}

