// block CP
inline void _CPU_::BLOCK_CP(bool increase, bool repeat) {
	byte val = RDMEM(R.HL.w);
	BLOCK_DELAY();
	byte res = R.AF.B.h - val;
	if (increase) R.HL.w++; else R.HL.w--; 
	R.BC.w--;
	R.AF.B.l = (R.AF.B.l & C_FLAG) |
		   ((R.AF.B.h ^ val ^ res) & H_FLAG) |
		   ZSTable[res] |
		   N_FLAG;
	if (R.AF.B.l & H_FLAG) res -= 1;
	if (res & 0x02) R.AF.B.l |= Y_FLAG; // bit 1 -> flag 5
	if (res & 0x08) R.AF.B.l |= X_FLAG; // bit 3 -> flag 3
	if (R.BC.w)     R.AF.B.l |= V_FLAG;
	if (repeat && R.BC.w && !(R.AF.B.l & Z_FLAG)) {
		BLOCK_DELAY(); R.PC.w -= 2;
	}
}
void _CPU_::cpd() {
	BLOCK_CP(false, false);
}
void _CPU_::cpi() {
	BLOCK_CP(true, false);
}
void _CPU_::cpdr() {
	BLOCK_CP(false, true);
}
void _CPU_::cpir() {
	BLOCK_CP(true, true);
}


// block LD
inline void _CPU_::BLOCK_LD(bool increase, bool repeat) {
	byte val = RDMEM(R.HL.w);
	WRMEM(R.DE.w, val);
	LDI_DELAY();
	R.AF.B.l &= S_FLAG | Z_FLAG | C_FLAG;
	if ((R.AF.B.h + val) & 0x02) R.AF.B.l |= Y_FLAG;	// bit 1 -> flag 5
	if ((R.AF.B.h + val) & 0x08) R.AF.B.l |= X_FLAG;	// bit 3 -> flag 3
	if (increase) { R.HL.w++; R.DE.w++; } else { R.HL.w--; R.DE.w--; }
	R.BC.w--;
	if (R.BC.w) R.AF.B.l |= V_FLAG;
	if (repeat && R.BC.w) { BLOCK_DELAY(); R.PC.w -= 2; }
}
void _CPU_::ldd() {
	BLOCK_LD(false, false);
}
void _CPU_::ldi() {
	BLOCK_LD(true, false);
}
void _CPU_::lddr() {
	BLOCK_LD(false, true);
}
void _CPU_::ldir() {
	BLOCK_LD(true, true);
}


// block IN
inline void _CPU_::BLOCK_IN(bool increase, bool repeat) {
	SMALL_DELAY();
	byte val = READ_PORT(R.BC.w);
	R.BC.B.h--;
	WRMEM(R.HL.w, val);
	if (increase) R.HL.w++; else R.HL.w--; 
	R.AF.B.l = ZSTable[R.BC.B.h];
	if (val & S_FLAG) R.AF.B.l |= N_FLAG;
	int k = val + ((R.BC.B.l + (increase ? 1 : -1)) & 0xFF);
	if (k & 0x100) R.AF.B.l |= H_FLAG | C_FLAG;
	R.AF.B.l |= ZSPXYTable[(k & 0x07) ^ R.BC.B.h] & P_FLAG;
	if (repeat && R.BC.B.h) { BLOCK_DELAY(); R.PC.w -= 2; }
}
void _CPU_::ind() {
	BLOCK_IN(false, false);
}
void _CPU_::ini() {
	BLOCK_IN(true, false);
}
void _CPU_::indr() {
	BLOCK_IN(false, true);
}
void _CPU_::inir() {
	BLOCK_IN(true, true);
}


// block OUT
inline void _CPU_::BLOCK_OUT(bool increase, bool repeat) {
	SMALL_DELAY();
	byte val = RDMEM(R.HL.w);
	if (increase) R.HL.w++; else R.HL.w--; 
	R.BC.B.h--;
	WRITE_PORT(R.BC.w, val);
	R.AF.B.l = ZSXYTable[R.BC.B.h];
	if (val & S_FLAG) R.AF.B.l |= N_FLAG;
	int k = val + R.HL.B.l;
	if (k & 0x100) R.AF.B.l |= H_FLAG | C_FLAG;
	R.AF.B.l |= ZSPXYTable[(k & 0x07) ^ R.BC.B.h] & P_FLAG;
	if (repeat && R.BC.B.h) { BLOCK_DELAY(); R.PC.w -= 2; }
}
void _CPU_::outd() {
	BLOCK_OUT(false, false);
}
void _CPU_::outi() {
	BLOCK_OUT(true, false);
}
void _CPU_::otdr() {
	BLOCK_OUT(false, true);
}
